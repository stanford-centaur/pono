MODULE main

IVAR
  data : unsigned word[8];

VAR 
  y: boolean;
  x_A: boolean;
  x.y : unsigned word[4] ;
  mem : array word[4] of word[8] ;

FROZENVAR
  y_A : unsigned word[4] ;

INIT 
  y_A = 0h4_9;

DEFINE
  x_y :=  x.y = 0ud4_0;

ASSIGN
  init(y) := TRUE;

TRANS 
  next(mem) = WRITE(mem, y_A, data) ;
  next(x.y) = y_A + 0ud4_011;

INVARSPEC x_y ;

